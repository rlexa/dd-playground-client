{
  "name": "dd-playground-client",
  "version": "10.0.0",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "show_graph": "dd-angular-graph --open-svg",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.1.0",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "d3": "^7.0.0",
    "dd-rxjs": "^1.5.0",
    "font-awesome": "^4.7.0",
    "pdfmake": "^0.1.68",
    "rxjs": "^6.5.5",
    "ts-mockery": "^1.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "^12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@types/d3": "^7.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.11.1",
    "@types/pdfmake": "^0.1.15",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "dd-angular-graph": "^1.0.4",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "material-design-icons": "^3.0.1",
    "ng-mocks": "^12.2.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.3.4",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
