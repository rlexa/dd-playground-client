{
  "name": "dd-playground-client",
  "version": "17.0.0",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng lint",
    "post:lint": "npm run lint:scss",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "d3": "^7.3.0",
    "dd-rxjs": "^1.5.1",
    "font-awesome": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "pdfmake": "^0.2.8",
    "rxjs": "^7.5.4",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@angular/language-service": "^17.3.9",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^18.18.13",
    "@types/pdfmake": "^0.2.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.53.0",
    "https-proxy-agent": "^7.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "ng-mocks": "^14.12.1",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
