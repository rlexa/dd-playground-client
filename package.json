{
  "name": "dd-playground-client",
  "version": "0.0.1",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "show_graph": "dd-angular-graph --open-svg",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --stats-json",
    "_test": "ng test (Karma) not working now because for jest removed: @types/jasminewd2 @types/jasmine",
    "test": "jest --ci --logHeapUsage",
    "test:watch": "jest --watchAll",
    "test:wu": "jest --watchAll -u",
    "test:watch:single": "jest --watchAll --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest-setup.ts",
    "testURL": "http://localhost/",
    "modulePathIgnorePatterns": [
      "<rootDir>.DS_Store",
      "<rootDir>/src/assets/Thumbs.db",
      "<rootDir>/.svn",
      "<rootDir>/.vscode",
      "<rootDir>/.sass-cache",
      "<rootDir>/coverage",
      "<rootDir>/dist",
      "<rootDir>/e2e",
      "<rootDir>/junit",
      "<rootDir>/node_modules",
      "<rootDir>/out-tsc",
      "<rootDir>/sass-binaries",
      "<rootDir>/tmp"
    ],
    "roots": [
      "<rootDir>/src/app"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@tensorflow/tfjs": "^0.14.1",
    "core-js": "^2.5.7",
    "d3": "^5.8.0",
    "dd-rx-state": "^1.0.1",
    "dd-rxjs": "^1.1.9",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/d3": "^5.0.0",
    "@types/jest": "^22.2.3",
    "@types/node": "~6.0.60",
    "codelyzer": "^5.0.1",
    "copyfiles": "^2.0.0",
    "dd-angular-graph": "^1.0.4",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.4",
    "jest-preset-angular": "^5.2.3",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons": "^3.0.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
