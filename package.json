{
  "name": "dd-playground-client",
  "version": "13.0.0",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng lint",
    "post:lint": "npm run lint:scss",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.4",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "d3": "^7.3.0",
    "dd-rxjs": "^1.5.1",
    "font-awesome": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "pdfmake": "^0.2.4",
    "rxjs": "^7.5.4",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular-eslint/builder": "^16.0.3",
    "@angular-eslint/eslint-plugin": "^16.0.3",
    "@angular-eslint/eslint-plugin-template": "^16.0.3",
    "@angular-eslint/schematics": "^16.0.3",
    "@angular-eslint/template-parser": "^16.0.3",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@angular/language-service": "^16.1.4",
    "@types/d3": "^7.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.19",
    "@types/pdfmake": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "https-proxy-agent": "^7.0.0",
    "jest": "^29.6.1",
    "jest-preset-angular": "^13.1.1",
    "ng-mocks": "^14.11.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "stylelint": "^15.10.1",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
