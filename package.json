{
  "name": "dd-playground-client",
  "version": "0.0.1",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --stats-json",
    "test": "jest --ci --logHeapUsage",
    "test:watch": "jest --watchAll",
    "test:watch:single": "jest --watchAll --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest-setup.ts",
    "modulePathIgnorePatterns": [
      "<rootDir>.DS_Store",
      "<rootDir>/src/assets/Thumbs.db",
      "<rootDir>/.svn",
      "<rootDir>/.vscode",
      "<rootDir>/.sass-cache",
      "<rootDir>/coverage",
      "<rootDir>/dist",
      "<rootDir>/e2e",
      "<rootDir>/junit",
      "<rootDir>/node_modules",
      "<rootDir>/out-tsc",
      "<rootDir>/sass-binaries",
      "<rootDir>/tmp"
    ],
    "roots": [
      "<rootDir>/src/app"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@tensorflow/tfjs": "^0.8.0",
    "@types/d3": "^5.0.0",
    "core-js": "^2.4.1",
    "d3": "^5.0.0",
    "dd-flexbox": "^0.9.2",
    "dd-virt-list": "^0.9.18",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "redux": "^3.7.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^22.2.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "copyfiles": "^2.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.3",
    "jest-preset-angular": "^5.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons": "^3.0.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
