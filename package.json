{
  "name": "dd-playground-client",
  "version": "9.0.0",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "show_graph": "dd-angular-graph --open-svg",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --stats-json",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "name": "dd-playground-client",
    "coverageDirectory": "dist/coverage",
    "collectCoverageFrom": [
      "src/app/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".module.ts",
      "index.ts",
      ".stories.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".stories.ts",
      "test.ts"
    ],
    "testURL": "http://localhost/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "d3": "^5.8.0",
    "dd-rx-state": "^1.1.0",
    "dd-rxjs": "^1.4.0",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.5.5",
    "ts-mockery": "^1.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@types/d3": "^5.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.0.0",
    "dd-angular-graph": "^1.0.4",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.0.0",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "material-design-icons": "^3.0.1",
    "ng-mocks": "^9.5.0",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
