{
  "name": "dd-playground-client",
  "version": "13.0.0",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "_view_webpack_stats": "webpack-bundle-analyzer dist/stats.json",
    "_prepare_assets": "copyfiles -f node_modules/material-design-icons/iconfont/*.{css,eot,svg,ttf,woff,woff2} src/assets",
    "_prep": "npm run _prepare_assets",
    "ng": "ng",
    "show_graph": "dd-angular-graph --open-svg",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng lint",
    "post:lint": "npm run lint:scss",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "d3": "^7.3.0",
    "dd-rxjs": "^1.5.0",
    "font-awesome": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "pdfmake": "^0.2.4",
    "rxjs": "^7.5.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.20.46",
    "@types/pdfmake": "^0.1.20",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "dd-angular-graph": "^1.0.4",
    "eslint": "^8.23.0",
    "https-proxy-agent": "^5.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "ng-mocks": "^14.11.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "stylelint": "^14.5.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-mockery": "^1.2.0",
    "ts-node": "^8.10.2",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}